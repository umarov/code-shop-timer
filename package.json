{
  "name": "code-shop-timer",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs && cp docs/index.html docs/404.html",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "firebase": "5.8.2",
    "register-service-worker": "^1.6.0",
    "vue": "^2.6.7",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-pwa": "^3.4.1",
    "@vue/cli-plugin-typescript": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "typescript": "^3.3.3",
    "vue-template-compiler": "^2.6.7"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
