{
  "name": "code-shop-timer",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "firebase": "^4.12.0",
    "register-service-worker": "^1.1.1",
    "vue": "^2.5.16",
